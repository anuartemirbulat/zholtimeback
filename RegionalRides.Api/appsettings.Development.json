{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RegionalRidesConnection": "User ID =postgres;Password=Parol12!@;Host=localhost;Port=5432;Database=RegionalRidesDevDb;Pooling=true;"
  },
  "Ik": {
    "Url": "http://10.10.10.10:5000"
  },
  "RabbitConfig": {
    "UserName": "user0",
    "Password": "user0",
    "HostName": "rabbitmq://localhost",
    "VirtualHostName": "dev-host"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Exceptions"
    ],
    "LogGroup": "IdentityApi.LogGroup",
    "LogStreamNamePrefix": "dev",
    "Region": "ap-south-1",
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithExceptionDetails"
    ],
    "Properties": {
      "Application": "IdentityApi"
    }
  },
}
